<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.ncu.newmedia.backschool.dao.FeedBackDao">
    <insert id="save" useGeneratedKeys="true" keyProperty="feedback.id">
        insert into feedback values (#{feedback.id},#{feedback.apply},
                                     #{feedback.level},#{feedback.filePath});
    </insert>
</mapper>